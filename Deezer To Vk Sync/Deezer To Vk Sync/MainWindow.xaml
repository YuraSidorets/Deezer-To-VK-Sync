<controls:MetroWindow x:Class="Deezer_To_Vk_Sync.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Deezer_To_Vk_Sync"
        xmlns:controls="http://metro.mahapps.com/winfx/xaml/controls"
        xmlns:forms="clr-namespace:System.Windows.Forms;assembly=System.Windows.Forms"
        xmlns:deezer="clr-namespace:Deezer_To_Vk_Sync.Deezer"
        mc:Ignorable="d"
        Title="Deezer To Vk Sync" Height="527" Width="684"
        GlowBrush="{DynamicResource FlatButtonBackgroundBrush}">

    <Grid Loaded="FrameworkElement_OnLoaded" Margin="0,0,0,-1">
        <Button x:Name="button" Content="Start" HorizontalAlignment="Left" Margin="20,20,0,0" VerticalAlignment="Top" Width="60" Height="40" Style="{StaticResource {x:Type Button}}" Click="Button_OnClick"/>
        <Button x:Name="button1" Content="Get Token" Margin="580,21,10,0" Style="{StaticResource {x:Type Button}}" Click="Button1_OnClick" Height="41" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="115,29,0,0" TextWrapping="Wrap" Text="Token" Width="445" Height="31" VerticalAlignment="Top"/>
        <WebBrowser x:Name="Browser" LoadCompleted="Browser_OnLoadCompleted"  />

    </Grid>
</controls:MetroWindow>
